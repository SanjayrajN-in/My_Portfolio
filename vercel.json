{
  "version": 2,
  "functions": {
    "api/auth/google.js": {
      "runtime": "@vercel/node"
    },
    "api/auth/login.js": {
      "runtime": "@vercel/node"
    },
    "api/auth/register.js": {
      "runtime": "@vercel/node"
    },
    "api/contact/submit.js": {
      "runtime": "@vercel/node"
    },
    "api/users/update-game-stats.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/auth/google/callback",
      "dest": "/auth/google/callback.html"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://sanjayrajn.vercel.app"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        },
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        }
      ]
    }
  ]
}